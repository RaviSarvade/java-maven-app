    stages {
        stage ("building") {
            steps {
                echo "its in building stage"
            }
            
        }
        stage ("testing") {
            steps {
                echo "its in testing stage"
            }
        }
        stage ("deploying") {
            steps {
                echo "its in deploying stage"
            }
        }     
    }
}
